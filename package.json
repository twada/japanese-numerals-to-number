  "version": "0.0.0",
  "devDependencies": {
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2",
    "semistandard": "^9.2.1",
    "snazzy": "^6.0.0"
  },
  "keywords": [
    "japanese",
    "numeral",
    "kansuji",
    "number"
